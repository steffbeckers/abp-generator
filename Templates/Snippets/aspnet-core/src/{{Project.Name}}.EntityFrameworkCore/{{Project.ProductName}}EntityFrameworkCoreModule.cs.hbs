{
  "RegexPattern": "(using Microsoft.Extensions.DependencyInjection;)",
  "RegexReplacement": "{0}\n$1"
}
#-#-#
using {{Project.Name}}.{{AggregateRoot.NamePlural}};
#-#-#
{
  "RegexPattern": "(options.AddDefaultRepositories\\\\(\\\\);)",
  "RegexReplacement": "$1\n\n{0}"
}
#-#-#
            options.AddRepository<{{AggregateRoot.Name}}, EfCore{{AggregateRoot.Name}}Repository>();
#-#-#
{
  "RegexPattern": "(Configure<AbpEntityOptions>\\\\(options =>
        {)",
  "RegexReplacement": "$1\n{0}"
}
#-#-#
{{#if AggregateRoot.Entities}}
            options.Entity<{{AggregateRoot.Name}}>(e =>
            {
                e.DefaultWithDetailsFunc = query => query
                    {{#each AggregateRoot.Entities}}
                    .Include(x => x.{{AggregateRootPropertyNamePlural}})
                    {{/each}};
            });
{{/if}}
